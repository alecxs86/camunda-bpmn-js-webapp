    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^5.0.0",
    "less": "^4.1.2",
    "min-dom": "^3.1.3",
    "mocha": "^9.2.0",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "preact": "^10.6.6",
    "puppeteer": "^13.5.0",
    "raw-loader": "^4.0.2",
    "rollup": "^2.38.4",
