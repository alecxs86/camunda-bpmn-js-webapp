{
  "name": "camunda-bpmn-js-webapp",
  "version": "0.0.1-alpha.3",
  "description": "Embeddable Camunda modeling distributions based on bpmn-js",
  "scripts": {
    "all": "run-s lint dev",
    "lint": "eslint .",
    "dev": "npm test -- --auto-watch --no-single-run",
    "start": "run-s bundle serve",
    "bundle": "run-s bundle:*",
    "bundle:js": "webpack -c webpack.config.js",
    "bundle:less": "lessc src/app.less public/app.css",
    "serve": "sirv public --dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/camunda/camunda-bpmn-js"
  },
  "keywords": [
    "bpmn",
    "bpmn20",
    "camunda",
    "zeebe",
    "modeling",
    "bpmn-js"
  ],
  "author": {
    "name": "Niklas Kiefer",
    "url": "https://github.com/pinussilvestrus"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "camunda-bpmn-js": "^0.12.2",
    "css-loader": "^6.7.0",
    "default-passive-events": "^2.0.0",
    "inherits": "^2.0.4",
    "jquery": "^3.5.1",
    "less-loader": "^10.2.0",
    "min-dash": "^3.8.1",
    "postcss-loader": "^6.2.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "transform-loader": "^0.2.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-bpmn-io": "^0.13.0",
    "execa": "^5.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^6.3.16",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
